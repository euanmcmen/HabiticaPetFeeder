{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Warning"
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Warning"
      }
    }
  },
  "AllowedHosts": "*",
  "HabiticaApiClientHeader": {
    "MyUserId": "hidden",
    "MyAppName": "hidden"
  },
  "HabiticaApi": {
    "UseLiveFetchEndpoint": true,
    "UseLiveFeedEndpoint": false,
    "RateLimitStandardDurationSeconds": 1,
    "RateLimitThrottleThreshold": 25,
    "RateLimitThrottleDurationSeconds": 3
  },
  "Encryption": {
    "Secret": "hidden"
  },
  "ConnectionStrings": {
    "MongoDbConnectionString": "hidden"
  }
}
